<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="165.0" prefWidth="325.0"
            xmlns="http://javafx.com/javafx/8.0.211"
            fx:controller="it.polimi.se2019.view.client.gui.ConnectionController">
    <children>
        <Button fx:id="RMIButton" layoutX="57.0" layoutY="72.0" mnemonicParsing="false"
                onAction="#startConnectionWithRMI" prefHeight="25.0" prefWidth="45.0" text="RMI"/>
        <Button fx:id="SocketButton" layoutX="216.0" layoutY="72.0" mnemonicParsing="false"
                onAction="#startConnectionWithSocket" text="Socket"/>
        <Text layoutX="79.0" layoutY="43.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Choose the type of connection"/>
    </children>
</AnchorPane>
