<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="it.polimi.se2019.view.client.gui.EndGameController">
    <children>
        <Label layoutX="62.0" layoutY="33.0" text="GAME OVER">
            <font>
                <Font name="System Bold" size="24.0"/>
            </font>
        </Label>
        <TableView fx:id="tablePoints" layoutX="51.0" layoutY="84.0" prefHeight="251.0" prefWidth="500.0">
            <columns>
                <TableColumn fx:id="rankColumn" prefWidth="75.0" text="Rank"/>
                <TableColumn fx:id="nicknameColumn" prefWidth="314.0" text="Nickname"/>
                <TableColumn fx:id="pointsColumn" minWidth="0.0" prefWidth="110.0" text="Points"/>
            </columns>
        </TableView>
        <Button fx:id="doneButton" layoutX="493.0" layoutY="353.0" mnemonicParsing="false" onAction="#closeStage"
                text="Done"/>
    </children>
</AnchorPane>
